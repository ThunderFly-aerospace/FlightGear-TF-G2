<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y">-3</chase-distance-m>

  <view n="100">
    <name>Mast Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <x-offset-m archive="y">  0.00000 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  0.24681 </y-offset-m> <!--    Up -->
      <z-offset-m archive="y">  0.64873 </z-offset-m> <!--  Back -->
      <pitch-offset-deg type="double">-25</pitch-offset-deg>
      <default-field-of-view-deg type="double">95</default-field-of-view-deg>
    </config>
  </view>

  <view n="101">
    <name>Tail Camera View</name>
    <type>lookfrom</type>
    <internal archive="y">true</internal>
    <config>
      <from-model type="bool">true</from-model>
      <from-model-idx type="int">0</from-model-idx>
      <ground-level-nearplane-m type="double">0.01f</ground-level-nearplane-m>
      <x-offset-m archive="y">  0.00000 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  0.24681 </y-offset-m> <!--    Up -->
      <z-offset-m archive="y">  0.64873 </z-offset-m> <!--  Back -->
      <default-field-of-view-deg type="double">30</default-field-of-view-deg>
      <pitch-offset-deg type="double">-5</pitch-offset-deg>
    </config>
  </view>
